	<!-- 
		List of healMeta fields to be stored in DC internally: 
		heal.type -> dc.type
		heal.title -> dc.title
		heal.creatorName -> dc.contributor.author
		heal.generalDescription -> dc.description
		heal.keyword.other -> dc.subject
		heal.spatialCoverage -> dc.coverage.spatial
		heal.temporalCoverage -> dc.coverage.temporal
		heal.identifierPrimary -> dc.identifier.uri
		heal.dateAvailable -> dc.date.available
		heal.language -> dc.language.iso
		heal.license -> dc.rights.uri, dc.rights.license
		heal.isReferencedBy -> dc.relation.isreferencedby
		heal.fileFormat -> dc.format.mimetype
		heal.publicationDate -> dc.date.issued
		heal.bibliographicCitation -> dc.identifier.citation
		heal.abstract -> dc.description.abstract
		heal.tableOfContents -> dc.description.tableofcontents
		heal.sponsor -> dc.description.sponsorship
		heal.advisorName -> dc.contributor.advisor
		heal.numberOfPages -> dc.format.extent
		heal.publisher -> dc.publisher
		heal.bookSeries -> dc.relation.ispartofseries
		heal.isPartOf -> dc.relation.ispartof
		heal.hasPart -> dc.relation.haspart
		heal.requires -> dc.relation.requires
		heal.isRelatedTo -> dc.relation
		heal.dateCreated -> dc.date.issued
		
		Qualified fields where qualifiers do not appear in healMeta:
		heal.classificationURI
		heal.keywordURI
	-->

